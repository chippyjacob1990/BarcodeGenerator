<?php

/**
 * @file
 * Primary module hooks for Sessions core module.
 *
 * @DCG
 * This file is no longer required in Drupal 8.
 * @see https://www.drupal.org/node/2217931
 */
function sessions_core_preprocess_block(&$variables) {
  if ($variables['elements']['#id'] == 'YOURBLOCKID') {
    $variables['#attached']['library'][] = 'sessions_core/barcode';
  }
}